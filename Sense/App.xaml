<Application x:Class="Sense.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewModels="clr-namespace:Sense.ViewModels"
             xmlns:converters="clr-namespace:Sense.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <viewModels:ViewModelLocator x:Key="ViewModelLocator" />
        <converters:PointYConverter x:Key="PointYConverter" />
        <converters:PointXConverter x:Key="PointXConverter" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    </Application.Resources>
</Application>
